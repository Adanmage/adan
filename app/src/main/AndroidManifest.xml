<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".UniteApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver
            android:name=".clockpackage.AlarmReceiver"
            android:enabled="true"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity android:name=".calendarpackage.WeekViewActivity" />
        <activity android:name=".calendarpackage.EventEditActivity" />
        <activity android:name=".calendarpackage.DailyCalendarActivity"/>
        <activity android:name=".MonthChartActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".RecordActivity" />
        <activity android:name=".clockpackage.clockmain" />
        <activity android:name=".notepackage.Notemain" />
        <activity android:name=".notepackage.AddNoteActivity"/>
        <activity android:name=".calendarpackage.Calendarmain"/>
        <activity android:name=".MainActivityM1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".clockpackage.AlarmForegroundService"
            android:enabled="true"
            android:exported="false" />


    </application>

</manifest>